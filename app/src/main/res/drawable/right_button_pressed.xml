<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size android:width="200px" android:height="200px"/>
            <solid android:color="@color/transparent"/>
            <stroke android:color="@color/pressed" android:width="5dp"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/right_arrow_pressed"
        android:bottom="20dp"
        android:left="20dp"
        android:right="15dp"
        android:top="20dp"
        />
</layer-list>